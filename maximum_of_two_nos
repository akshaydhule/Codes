//============================================================================
// Name        : maxoftwono.cpp
// Description : return the maximum of two numbers without library functions
//============================================================================

#include <iostream>
using namespace std;

void max(int a, int b)
{
	int c = a-b;
	c = (c>>31) & 0x1;
	int q = 1^c;
	int d = a*q+b*c;
	cout << d;
}
int main() {
	int a=21,b=31;
	max(a,b);
	return 0;
}
