#include <iostream>
#include <cstdlib>
using namespace std;

struct node 
{
	int val;
	struct node * next;
};


void reverse(struct node * head)
{
	if (head->next!=NULL)
	{
		reverse(head->next);
	}
	cout<<head->val;
}

int main() {
	struct node * head = (struct node *) malloc (sizeof (struct node ));
	head ->val = 1;
	struct node * first = (struct node *) malloc (sizeof (struct node ));
	first->val = 2;
	head->next = first;
	struct node * second = (struct node *) malloc (sizeof (struct node ));
	second->val = 3;
	first->next = second;
	second->next = NULL;
	reverse(head);
	return 0;
}
